        "meta_description": "Find Jewish classics with wine and cocktails at Belle’s Delicatessen & Bar",
        "meta_lang": "en",
        "meta_favicon": "https://cdn.vox-cdn.com/uploads/chorus_asset/file/6253165/favicon.0.ico",
